{
  "name": "berthott/laravel-api-permissions",
  "description": "Laravel Helper for API Endpoint Permissions",
  "license": "MIT",
  "authors": [
      {
          "name": "Jan Bladt",
          "email": "berthott@web.de"
      }
  ],
  "minimum-stability": "dev",
  "autoload" : {
      "psr-4" : {
          "berthott\\Permissions\\": "src",
          "berthott\\Permissions\\Database\\Factories\\": "database/factories"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Database\\Seeders\\": "database/seeders",
          "berthott\\Permissions\\Tests\\": "tests"
      }
  },
  "extra" : {
      "laravel" : {
          "providers" : [
              "berthott\\Permissions\\ApiPermissionsServiceProvider"
          ]
      }
  },
  "require-dev": {
    "berthott/laravel-api-cache": "^3.0",
    "berthott/laravel-crudable": "^3.0",
    "orchestra/testbench": "^8.0",
    "phpunit/phpunit": "^10.0",
    "doctrine/dbal": "^3.3",
    "berthott/laravel-targetable": "^3.0"
  }
}
